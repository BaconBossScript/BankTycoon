 
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "BaconBoss Hub | ðŸ’° Bank Tycoon", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
 
_G.AutoFarm = true

function AutoFarm()
     while _G.AutoFarm == true do
     for i, part in pairs(game.Workspace.CrateParent:GetChildren()) do
        if part:IsA("BasePart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
            wait(0.1)
        end
     task.wait()
     end
    end
end

local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddToggle({
	Name = "Free Money",
	Default = false,
	Callback = function(Value)
		_G.AutoFarm = Value
        AutoFarm()
	end    
})

MainTab:AddButton({
	Name = "This Script For Collect Coins",
	Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
    end
}) 
